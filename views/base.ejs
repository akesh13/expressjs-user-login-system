<%-include('header')-%>
<div class="text-center center-div" id="login">
  <div class="container w-25 py-5">
    <div class="title pb-2">
      <h2 class="font-weight-bold">Login System</h2>
      <span>Login for the existing user</span>
    </div>
    <form class="form" action="/route/login" method="POST">
      <div class="form-group">
        <input type="email" class="form-control pb-2" placeholder="E-mail" name="email"autocomplete="off">
        <small class="form-text text-muted text-left pb-2">Register email address</small>
      </div>
      <div class="form-group pb-2">
        <input type="password" class="form-control pb-2" placeholder="Password" name="password">
      </div>
      <button class="btn btn-primary rounded-pill">Submit</button>
    </form>
  </div>
</div>

<%-include('footer')-%>

 
